{
  "name": "@gemeente-denhaag/denhaag-component-library",
  "version": "0.1.0",
  "private": true,
  "description": "A library of React components for the Design System of The Netherlands",
  "bugs": "https://github.com/nl-design-system/denhaag/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/nl-design-system/denhaag.git"
  },
  "license": "EUPL-1.2",
  "author": "Municipality of The Hague",
  "workspaces": [
    "src/components/*",
    "src/meta-packages/*",
    "src/styles/*",
    "src/"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "build-storybook": "build-storybook -s .storybook/stories/assets/",
    "clean": "lerna run clean --parallel && rimraf .rollup-cache",
    "lint": "npm-run-all --continue-on-error lint:* && lerna run lint",
    "lint-fix": "npm-run-all --continue-on-error lint-fix:* prettier",
    "lint-fix:css": "stylelint --fix src/**/*.css",
    "lint-fix:js": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "lint:css": "stylelint src/**/*.css",
    "lint:package": "npmPkgJsonLint src/**/package.json",
    "lint:js": "eslint src/**/*.{js,jsx,ts,tsx}",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --write .",
    "prettier-check": "prettier -c src/**/*.{js,jsx,ts,tsx}",
    "publish": "lerna publish from-package",
    "release": "lerna version prerelease --no-changelog --no-private --no-commit-hooks",
    "docker-dev": "npm-run-all --sequential bootstrap build storybook",
    "storybook": "npm-run-all --parallel watch:*",
    "test": "cypress run-ct",
    "test:open": "cypress open-ct",
    "watch:storybook": "start-storybook -p 6006 -s .storybook/stories/assets/",
    "watch:style-dictionary": "lerna run --parallel --scope '@gemeente-denhaag/design-tokens-*' watch:style-dictionary",
    "typecheck:storybook": "yarn tsc --project tsconfig.storybook.json --noEmit"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha",
    "@material-ui/pickers": "^3.2.10",
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@cypress/react": "^5.4.2",
    "@cypress/webpack-dev-server": "^1.1.6",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@storybook/addon-a11y": "^6.3.2",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addons": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "axe-core": "^4.2.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.1.0",
    "chokidar-cli": "^2.1.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.2",
    "cypress": "^8.0.0",
    "cypress-axe": "^0.12.2",
    "cypress-plugin-snapshots": "^1.4.4",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lerna": "^4.0.0",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.21",
    "mdx-embed": "^0.0.19",
    "npm-package-json-lint": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^2.1.1",
    "postcss": "^8.3.5",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.6",
    "rollup-plugin-import-css": "^2.0.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svgo": "^1.1.0",
    "storybook": "^6.3.2",
    "storybook-addon-themes": "^6.1.0",
    "storybook-design-token": "^1.1.1",
    "style-dictionary": "^3.0.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "typescript": "^4.1.3",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": ">=10",
    "yarn": "^1.22.0"
  }
}
